OBJS=$(Src_C:.C=.o) $(Src_c:.c=.o)

COMMON_FLAGS = -g -I. -I../include -I../common $(MY_FLAGS)

all:$(OBJS)
	@echo "building $(Exe) ..."
	@g++ $(COMMON_FLAGS) $(MY_FLAGS) -o $(Exe) $(OBJS)
	@echo "--->>>>>>>>> MAKE OK <<<<<<<<<---"
	@del *.o
	@cd ../common/ && del *.o

clean:
	@del *.o
	@cd ../common/ && del *.o